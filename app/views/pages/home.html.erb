<h1>Fit-Link</h1>

    <% unless user_signed_in?%>

      <%#sign in%>
      <%= link_to 'Sign In', new_user_session_path %>
      
      <%#sign up%>
      <%= link_to 'Sign Up Trainer', new_profile_path %>
      <%= link_to 'Sign Up as Gym',  new_gym_path %>
    <%else%>

      <%# sign out%>
      <%= link_to 'Sign Out', destroy_user_session_path, method: :delete %>

    <% end %>