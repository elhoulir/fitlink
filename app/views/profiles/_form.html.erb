<%= form_with(model: profile, local: true) do |form| %>
  <% if profile.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(profile.errors.count, "error") %> prohibited this profile from being saved:</h2>

      <ul>
      <% profile.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :first_name %>
    <%= form.text_field :first_name, id: :profile_first_name %>
  </div>

  <div class="field">
    <%= form.label :last_name %>
    <%= form.text_field :last_name, id: :profile_last_name %>
  </div>

  <div class="field">
    <%= form.label :date_of_birth%>
    <%= form.text_field :date_of_birth, id: :profile_date_of_birth %>
  </div>

  <div class="field">
    <%= form.label :address %>
    <%= form.text_field :address, id: :profile_address %>
  </div>

  <div class="field">
    <%= form.label :contact_number %>
    <%= form.text_field :contact_number, id: :profile_contact_number %>
  </div>

  <div class="field">
    <%= form.label :license_number %>
    <%= form.text_field :license, id: :profile_license %>
  </div>

  <div class="field">
    <%= form.label :license_image %>
    <%= form.file_field :license_image, id: :profile_license_image %>
  </div>

  <div class="field">
    <%= form.label :photo %>
    <%= form.file_field :photo, id: :profile_photo %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>