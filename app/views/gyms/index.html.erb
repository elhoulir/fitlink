<p id="notice"><%= notice %></p>

<h1>Gyms</h1>

<table class="details">
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Abn</th>
      <th>Address</th>
      <th>Contact number</th>
      <th>Image data</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @gyms.each do |gym| %>
      <tr>
        <td><%= gym.name %></td>
        <td><%= gym.description %></td>
        <td><%= gym.abn %></td>
        <td><%= gym.address %></td>
        <td><%= gym.contact_number %></td>
        <td><%= gym.image_data %></td>
        <td><%= gym.user %></td>
        <td><%= link_to 'Show', gym %></td>
        <td><%= link_to 'Edit', edit_gym_path(gym) %></td>
        <td><%= link_to 'Destroy', gym, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Gym', new_gym_path %>
